{
  "name": "check spell",
  "version": "1.0",
  "description": "Build an Extension!",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": {
    "matches": ["http://*/*", "https://*/*"],
    "js": ["contentscript.js"]
  },
  "permissions": ["storage"],
  "browser_action": {
    "default_icon": "favicon.png",
    "default_popup": "popup.html"
  }
}
